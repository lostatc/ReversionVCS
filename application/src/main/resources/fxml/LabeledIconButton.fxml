<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXRippler?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Circle?>
<?import org.kordamp.ikonli.javafx.FontIcon?>
<fx:root xmlns:fx="http://javafx.com/fxml/1" type="JFXRippler" maskType="CIRCLE" xmlns="http://javafx.com/javafx/11.0.1"
         styleClass="icon-button">
    <VBox alignment="center">
        <StackPane>
            <FontIcon fx:id="fontIcon" styleClass="button-icon"/>
            <Circle fx:id="foreground" styleClass="button-foreground" fill="TRANSPARENT" stroke="TRANSPARENT"
                    strokeType="INSIDE"/>
        </StackPane>
        <Label fx:id="buttonLabel" styleClass="button-label"/>
    </VBox>
</fx:root>
