<?xml version="1.0" encoding="UTF-8"?>

<?import io.github.lostatc.reversion.gui.controls.IconButton?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<fx:root xmlns:fx="http://javafx.com/fxml/1" type="StackPane" xmlns="http://javafx.com/javafx/11.0.1">
    <AnchorPane fx:id="buttonPane" pickOnBounds="false">
        <IconButton fx:id="helpButton" icon="gmi-help" onAction="#openHelp" styleClass="icon-button-light"/>
        <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
        </padding>
    </AnchorPane>
</fx:root>
